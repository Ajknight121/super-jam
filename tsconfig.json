{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "types": ["./worker-configuration.d.ts", "node"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "#/src/*": ["src/*"]
    },
    "lib": ["ES2021", "DOM"],
    "target": "ES2021"
  }
}
